<?xml version="1.0" encoding="UTF-8" ?>
<queries>
    <query id="getTodoByTagName">
        SELECT
        *
        FROM tag
        WHERE tag_name = ?
    </query>
    <query id="addTag">
        UPDATE INTO tag (tag_name,todo_id)
        VALUES(?,?)
    </query>
    <query id="addUser">
        INSERT INTO user (email,nickname,password)
        VALUES(?,?,?)
    </query>

</queries>